<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data noupdate="1">
        <record id="ir_cron_generate_supplier_journal_items_file" model="ir.cron">
            <field name="name">Export Supplier Invoice file</field>
            <field name="model_id" ref="model_account_move"/>
            <field name="type">ir.actions.server</field>
            <field name="state">code</field>
            <field name="code">model.cron_generate_supplier_journal_items_file()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
        </record>

        <record id="ir_cron_export_supplier_invoices" model="ir.cron">
            <field name="name">Send SUpplier Invoice to FTP server</field>
            <field name="model_id" ref="model_account_move"/>
            <field name="type">ir.actions.server</field>
            <field name="state">code</field>
            <field name="code">model.cron_send_supplier_invoice_to_ftp()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
        </record>
    </data>
</odoo>
