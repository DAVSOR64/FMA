<odoo>
    <record id="mrp_production_workorder_tree_editable_view_inherit_mrp_macro_planning" model="ir.ui.view">
        <field name="name">mrp.production.work.order.tree.editable.inherit</field>
        <field name="model">mrp.workorder</field>
        <field name="inherit_id" ref="mrp.mrp_production_workorder_tree_editable_view"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='product_id']" position="after">
                <field name="date_macro" optional="show"/>
            </xpath>
        </field>
    </record>
</odoo>
