<odoo>
    <data noupdate="1">
        <record id="ir_cron_generate_customer_files" model="ir.cron">
            <field name="name">Export supplier Files</field>
            <field name="model_id" ref="model_res_partner"/>
            <field name="type">ir.actions.server</field>
            <field name="state">code</field>
            <field name="code">model.cron_generate_generate_supplier_files()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
        </record>

        <record id="ir_cron_export_customer_files_to_sftp_server" model="ir.cron">
            <field name="name">Send Customer Files to SFTP server</field>
            <field name="model_id" ref="model_res_partner"/>
            <field name="type">ir.actions.server</field>
            <field name="state">code</field>
            <field name="code">model.cron_send_supplier_file_to_sftp_server()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
        </record>
    </data>
</odoo>
