<?xml version="1.0" encoding="UTF-8"?>
<odoo>
  <template id="purchase_order_export_template">
    <CCS xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
      <t t-set="po" t-value="po"/>
      <Internal Type="Order" Version="617" Build="2.46.241" Job="3175" ItemSet="0000003643">
        <PriceList>
          <PriceDate t-att-Name="po.partner_id.name" t-att-Date="po.create_date"/>
        </PriceList>
      </Internal>
      <Orders>
        <Order>
          <Header DocumentType="ZOR" PuchOrderType="Z008" BrandID="20" SalesSBUID="2000" CustomerNumber="130172" CustomerEmail="" BuildingProjectID="" t-att-DocumentNumber="po.name" t-att-DocumentDate="po.create_date" t-att-OrderReference="po.x_studio_rfrence" ShipToNumber="" PreferedLanguage="FR" WorkType="03" FinalCusttype="03">
            <Address/>
            <InternalRemarks></InternalRemarks>
          </Header>
          <Articles>
            <t t-set="count" t-value="10"/>
            <t t-foreach="po.order_line" t-as="line">
              <Article
                t-att-ItemCount="count"
                t-att-ArticleID="line.product_id.x_studio_ref_int_logikal"
                t-att-ArticlesQuantity="line.product_qty"
                t-att-SalesUnitOfMeasure="line.product_uom.name"
                t-att-Length="line.product_id.x_studio_largeur_mm"
                t-att-ExternalColor="line.product_id.x_studio_color_logikal"/>
              <t t-set="count" t-value="count + 10"/>
            </t>
          </Articles>
        </Order>
      </Orders>
    </CCS>
  </template>
</odoo>
