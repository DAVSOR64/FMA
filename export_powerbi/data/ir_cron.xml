<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data noupdate="1">
        <record id="ir_cron_generate_powerbi_files" model="ir.cron">
            <field name="name">Export power bi Files</field>
            <field name="model_id" ref="model_res_partner"/>
            <field name="type">ir.actions.server</field>
            <field name="state">code</field>
            <field name="code">model.cron_generate_generate_powerbi_files()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
        </record>

        <record id="ir_cron_export_powerbi_files_to_sftp_server" model="ir.cron">
            <field name="name">Send power bi Files to SFTP server</field>
            <field name="model_id" ref="model_res_partner"/>
            <field name="type">ir.actions.server</field>
            <field name="state">code</field>
            <field name="code">model.cron_send_powerbi_file_to_sftp_server()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
        </record>
    </data>
</odoo>
